<div class="container">

    <div class="col-md-8">
      <h1>List a new vaccine</h1>
      <div>
      <%= simple_form_for @vaccine do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      </div>
      <% end %>
    </div>

</div>
